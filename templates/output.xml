<templateSet group="output">
  <template name="lderr" value="LOG.error(&quot;$END$&quot;);" description="Prints a string to logger.error" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="false" />
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="ldouf" value="LOG.debug(&quot;$END$&quot;);" description="Prints a formatted string to logger.debug" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="false" />
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="ldout" value="LOG.debug($END$);" description="Prints a string to logger.debug" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="false" />
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="ldoutm" value="LOG.debug(&quot;$CLASS_NAME$.$METHOD_NAME$&quot;);" description="Prints current class and method names to logger.debug" toReformat="true" toShortenFQNames="true">
    <variable name="CLASS_NAME" expression="className()" defaultValue="" alwaysStopAt="false" />
    <variable name="METHOD_NAME" expression="methodName()" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="JAVA_CODE" value="false" />
      <option name="JAVA_STATEMENT" value="true" />
      <option name="GROOVY" value="false" />
      <option name="GROOVY_STATEMENT" value="false" />
    </context>
  </template>
  <template name="ldoutp" value="LOG.debug($FORMAT$);" description="Prints method parameter names and values to logger.debug" toReformat="true" toShortenFQNames="true">
    <variable name="FORMAT" expression="groovyScript(&quot;'\&quot;' + _1.collect { it + ' = [\&quot; + ' + it + ' + \&quot;]'}.join(', ') + '\&quot;'&quot;, methodParameters())" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="JAVA_CODE" value="false" />
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="ldoutv" value="LOG.debug(&quot;$EXPR_COPY$ = {}&quot;, $EXPR$);" description="Prints a value to logger.debug" toReformat="true" toShortenFQNames="true">
    <variable name="EXPR" expression="variableOfType(&quot;&quot;)" defaultValue="&quot;expr&quot;" alwaysStopAt="true" />
    <variable name="EXPR_COPY" expression="escapeString(EXPR)" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="JAVA_CODE" value="false" />
      <option name="JAVA_STATEMENT" value="true" />
      <option name="GROOVY" value="false" />
      <option name="GROOVY_STATEMENT" value="false" />
    </context>
  </template>
</templateSet>